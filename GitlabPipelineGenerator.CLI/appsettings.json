{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "Console": {
      "IncludeScopes": false,
      "TimestampFormat": "HH:mm:ss "
    }
  },
  "Pipeline": {
    "DefaultOutputPath": ".gitlab-ci.yml",
    "DefaultStages": [ "build", "test", "deploy" ],
    "SupportedProjectTypes": [ "dotnet", "nodejs", "python", "docker", "generic" ],
    "SupportedDotNetVersions": [ "6.0", "7.0", "8.0", "9.0" ]
  }
}